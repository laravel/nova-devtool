laravel: '@testbench'

providers:
  - Laravel\Nova\NovaCoreServiceProvider
  - {{ WorkbenchAppNamespace }}Providers\NovaServiceProvider
  # - {{ WorkbenchServiceProvider }}

migrations: true

seeders:
  - {{ WorkbenchDatabaseSeeder }}

workbench:
  start: /nova
  build:
    - package:discover
    - asset-publish
    - create-sqlite-db
    - db:wipe
    - migrate:refresh
  assets:
    - nova-assets
  sync: []

purge:
  directories:
    - lang/*
    - public/vendor/*
