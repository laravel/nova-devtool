{
    "name": "laravel/nova-devtool",
    "description": "Devtool for Laravel Nova Development",
    "type": "library",
    "keywords": ["laravel", "laravel-nova", "dev"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "mior@laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Nova\\DevTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Nova\\DevTool\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "require": {
        "php": "^8.0",
        "composer-runtime-api": "^2.2",
        "fakerphp/faker": "^1.21",
        "orchestra/workbench": "^7.0|^8.0|^9.0",
        "symfony/polyfill-php83": "^1.28"
    },
    "require-dev": {
        "larastan/larastan": "^2.4",
        "laravel/nova": "^4.34",
        "orchestra/testbench": "^7.39|^8.20|^9.0",
        "phpunit/phpunit": "<11.0.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-develop": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Nova\\DevTool\\LaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "pint",
            "@php vendor/bin/phpstan analyse"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
