{
    "name": "laravel/nova-devtool",
    "description": "Devtool for Laravel Nova Development",
    "type": "library",
    "keywords": ["laravel", "laravel-nova", "dev"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "mior@laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Nova\\DevTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Nova\\DevTool\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "require": {
        "php": "^8.1",
        "composer-runtime-api": "^2.2",
        "fakerphp/faker": "^1.21",
        "orchestra/workbench": "^8.5|^9.1",
        "symfony/polyfill-php83": "^1.28"
    },
    "require-dev": {
        "larastan/larastan": "^2.4",
        "laravel/nova": "dev-develop",
        "orchestra/testbench": "^8.24|^9.2",
        "phpstan/phpstan": "^1.11"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel/nova"
        }
    ],
    "config": {
        "preferred-install": {
            "laravel/nova": "source",
            "*": "auto"
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Nova\\DevTool\\LaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "pint",
            "@php vendor/bin/phpstan analyse"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
